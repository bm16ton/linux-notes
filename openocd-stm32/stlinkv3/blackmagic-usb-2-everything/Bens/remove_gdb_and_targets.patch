-static bool efm32_cmd_serial(target *t)
-{
-	/* Read the extended unique identifier */
-	uint64_t eui = efm32_read_eui(t);
-
-	/* 64 bits of unique number */
-	tc_printf(t, "Unique Number: 0x%016llx\n", eui);
-
-	return true;
-}
