-	f->write = target_flash_write_buffered;
-	f->done = target_flash_done_buffered;
-	f->write_buf = sam3x_flash_write;
-	f->buf_size = SAM3_PAGE_SIZE;
-	sf->eefc_base = eefc_base;
-	sf->write_cmd = EEFC_FCR_FCMD_EWP;
