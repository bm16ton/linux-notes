-static void stm32f4_flash_unlock(target *t)
-{
-	if (target_mem_read32(t, FLASH_CR) & FLASH_CR_LOCK) {
-		/* Enable FPEC controller access */
-		target_mem_write32(t, FLASH_KEYR, KEY1);
-		target_mem_write32(t, FLASH_KEYR, KEY2);
-	}
-}
