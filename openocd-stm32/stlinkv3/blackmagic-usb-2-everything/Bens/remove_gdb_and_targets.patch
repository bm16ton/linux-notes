-	/* If WDT on, pet */
-	if (wdt_mode) {
-		target_mem_write32(t, LPC43XX_WDT_FEED, 0xAA);
-		target_mem_write32(t, LPC43XX_WDT_FEED, 0xFF);
-	}
-}
