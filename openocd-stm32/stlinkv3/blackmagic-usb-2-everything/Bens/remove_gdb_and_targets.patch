-bool optcr_mask(target *t, uint32_t *val)
-{
-	switch (t->idcode) {
-	case ID_STM32F20X:
-	case ID_STM32F40X:
-		val[0] &= ~0xF0000010;
-		break;
-	case ID_STM32F46X:
-	case ID_STM32F42X:
-		val[0] &= ~0x30000000;
-		val[1] &=  0x0fff0000;
-		break;
-	case ID_STM32F401C:
-		val[0] &= ~0x7FC00010;
-		break;
-	case ID_STM32F446:
-	case ID_STM32F411:
-	case ID_STM32F401E:
-		val[0] &= ~0x7F000010;
-		break;
-	case ID_STM32F410:
-		val[0] &= ~0x7FE00010;
-		break;
-	case ID_STM32F412:
-		val[0] &= ~0x70000010;
-		break;
-	case ID_STM32F413:
-		val[0] &= ~0x00000010;
-		break;
-	case ID_STM32F72X:
-		val[2] &=  ~0x800000ff;
-		/* Fall through*/
-	case ID_STM32F74X:
-		val[0] &= ~0x3F000000;
-		break;
-	case ID_STM32F76X:
-		break;
-	default:
-		return false;
-	}
-	return true;
-}
