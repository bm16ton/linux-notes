-void morse(const char *msg, char repeat)
-{
-	morse_msg = morse_ptr = msg;
-	morse_repeat = repeat;
-}
-
-bool morse_update(void)
-{
-	static uint16_t code;
-	static uint8_t bits;
-
-	if (!morse_ptr)
-		return false;
-
-	if (!bits) {
-		char c = *morse_ptr++;
-		if (!c) {
-			if(morse_repeat) {
-				morse_ptr = morse_msg;
-				c = *morse_ptr++;
-			} else {
-				morse_ptr = 0;
-				return false;
-			}
-		}
-		if ((c >= 'A') && (c <= 'Z')) {
-			c -= 'A';
-			code = morse_letter[c].code;
-			bits = morse_letter[c].bits;
-		} else {
-			code = 0; bits = 4;
-		}
-	}
-
-	bool ret = code & 1;
-	code >>= 1;
-	bits--;
-
-	return ret;
-}
